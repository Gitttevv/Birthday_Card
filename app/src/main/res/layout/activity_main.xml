<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    app:flow_firstHorizontalStyle="packed"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="767dp"
        android:layout_height="759dp"
        android:layout_marginStart="4dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/dark"
        tools:layout_editor_absoluteY="-1dp" />

    <TextView
        android:layout_width="293dp"
        android:layout_height="341dp"
        android:layout_marginTop="548dp"
        android:fontFamily="cursive"
        android:layoutDirection="locale"
        android:rotationY="-17"
        android:shadowColor="#040303"
        android:text="Happy Birthday Kate"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#F0F3ED"
        android:textDirection="locale"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="710dp"
        android:layout_height="532dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/cake6"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="4dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="418dp"
        android:layout_height="357dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/pred33"
        tools:layout_editor_absoluteX="300dp"
        tools:layout_editor_absoluteY="534dp" />

</androidx.constraintlayout.widget.ConstraintLayout>